                .#{$prefix}frame-bl,
                .#{$prefix}frame-tr,
                .#{$prefix}frame-br,
                .#{$prefix}frame-tc,
                .#{$prefix}frame-bc {
                    @include theme-background-image($theme-name, 'btn/btn-#{$ui}-focus-corners.gif');
