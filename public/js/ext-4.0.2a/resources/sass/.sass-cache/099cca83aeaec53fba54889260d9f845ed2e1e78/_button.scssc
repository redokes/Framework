    .#{$prefix}btn-icon-text-top .#{$prefix}btn-icon {
        background-position: center top;
    }

    .#{$prefix}btn-icon-text-bottom .#{$prefix}btn-icon {
        background-position: center bottom;
    }

    .#{$prefix}btn {
        button, a {
            position: relative;

            .#{$prefix}btn-icon {
                position: absolute;
                background-repeat: no-repeat;
            }
        }
    }
  
    .#{$prefix}btn-arrow-right {
